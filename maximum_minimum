#include <stdio.h>
int main()
{
        int n;
        int a[n];
        scanf("%d",&n);
        for(int i=0;i<n;i++)
            scanf("%d",&a[i]);
        int max=a[0];
        int min=a[0];
        for(int i=0;i<n;i++)
            {if(a[i]>max)
            max=a[i];
            if(a[i]<min)
            min=a[i];
            }
         printf("max is %d\n",max);
         printf("min is %d",min);   
}
