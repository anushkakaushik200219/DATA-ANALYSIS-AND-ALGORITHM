#include <stdio.h>
int main()
{
        int n;
        int a[n];
        int s,pos=0;
        scanf("%d",&n);
        for(int i=0;i<n;i++)
            scanf("%d",&a[i]);
            scanf("%d",&s);
        for(int i=0;i<n;i++) 
        {
            if(a[i]==s)
              {
                 pos=i;
              }
        }
    
 if(pos==0)  
 {
     printf("element not found");
 }
 else
  {
      printf("element fount at position : %d",pos);
  }
} 
